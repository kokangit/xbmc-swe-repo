<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.dreamfilm" name="Dreamfilm" provider-name="Daniel Lundin" version="0.1.15.10">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension library="default.py" point="xbmc.python.pluginsource">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary>Stream from http://dreamfilmhd.org</summary>
    <description>Stream from http://dreamfilmhd.org</description>
  </extension>
</addon>
<addon id="repository.swe"
       name="swedish repository"
       version="0.1"
       provider-name="kokangit">
  <extension point="xbmc.addon.repository" name="Repository for swedish addons">
    <info compressed="false">https://raw.githubusercontent.com/kokangit/xbmc-swe-repo/master/addons.xml</info>
    <checksum>https://raw.githubusercontent.com/kokangit/xbmc-swe-repo/master/addons.xml.md5</checksum>
    <datadir zip="true">http://raw.github.com/kokangit/xbmc-swe-repo/master</datadir>
  </extension>
</addon>
</addons>
